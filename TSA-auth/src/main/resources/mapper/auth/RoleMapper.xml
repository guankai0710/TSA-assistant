<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ryan.tsa.auth.mapper.RoleMapper">

    <select id="getList" resultType="com.ryan.tsa.auth.vo.RoleVo">
        SELECT
            role_id as roleId,
            role_name as roleName,
            role_memo as roleMemo
        FROM
            tsa_auth_role
    </select>
</mapper>
